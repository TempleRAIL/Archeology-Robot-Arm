<?xml version="1.0" ?>
<sdf version="1.5">

<!-- Geometric constraints of model meshes removed in Blender
Inertials and bounding box size of model meshes calculated in Meshlab.
Mass derived from volume calculated in Meshlab and assumed density of 5000 kg/m^3
See Computing Inertial Parameters and Rescaling Inertia Values: http://gazebosim.org/tutorials?tut=inertia#Overview -->

  <model name='sherd_b'>
    <pose frame=''>0 0 0 0 0 0</pose>
    <static>false</static> <!-- mass is ignored for static models -->
    <link name='sherd_b_link'>
      <pose frame=''>0 0 0.005 0 0 0</pose>
      <inertial>
        <mass>0.266</mass>
        <inertia>
          <ixx>4.062088e-05</ixx>
          <ixy>-7.599255999999999e-06</ixy>
          <ixz>-1.1300795e-06</ixz>
          <iyy>0.00012027606949999999</iyy>
          <iyz>-3.0143439999999996e-06</iyz>
          <izz>0.00013008792099999998</izz>
        </inertia>
      </inertial>
      <collision name='collision'> <!-- .dae mesh for collision falls through the blue mat; .stl avoids this -->
        <geometry>
          <!-- <box>
            <size>0.081363 0.047989 0.036223</size>
          </box> -->
          <mesh>
            <!-- <uri>model://sherd_b/meshes/sherd_b.dae</uri> -->
            <uri>model://sherd_b/meshes/sherd_b.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <surface>
          <contact>
            <poissons_ratio>0.15</poissons_ratio>
            <elastic_modulus>8e+10</elastic_modulus>
            <ode>
              <kp>100000</kp>
              <kd>100</kd>
              <max_vel>100</max_vel>
              <min_depth>0.001</min_depth>
            </ode>
          </contact>
          <friction>
            <torsional>
              <coefficient>1</coefficient>
            </torsional>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
              <fdir1>1 0 0</fdir1> <!-- specify in x dir to prevent slipping on mat AND in gripper. Gazebo automatically picks fdir2 as vector normal to applied force on surface -->
            </ode>
          </friction>
          <bounce/>
        </surface>
        <max_contacts>10</max_contacts>
      </collision>
      <visual name='visual'>
        <geometry>
          <mesh>
            <!-- <uri>model://sherd_b/meshes/sherd_b.dae</uri> -->
            <uri>model://sherd_b/meshes/sherd_b.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
          <!--<box>
            <size>0.07 0.025 0.01</size>
          </box>-->
        </geometry>
        <material>
          <script>
            <uri>model://sherd_b/materials/scripts</uri>
            <uri>model://sherd_b/materials/textures</uri>
            <name>sherdMat/b</name>
          </script>
        </material>
      </visual>
      <self_collide>0</self_collide>
      <kinematic>0</kinematic>
      <gravity>1</gravity>
    </link>
  </model>
</sdf>
